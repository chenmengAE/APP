/* pages/regest/regest.wxss */
.coatain{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  position: relative;
}
image{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.title{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: block;
  border-radius: 6px;
  text-align: center;
  font-size: 20px;
  color: white;
}
.from{
  margin-top: 100rpx;
  padding: 20rpx 0 0 0;
}

label{
  display: flex;
  justify-content: flex-start;
  width: 720rpx;
  margin: 10rpx 0;
  margin-left: 15rpx;
}
label text{
  width: 150rpx;
  display: block;
  line-height: 80rpx;
  font-size: 18px;
  color: white;
}
label switch{
  margin: 5px 5px 0 63px;
}
label input{
  flex: 1;
  padding: 0 0 0 20rpx;
  height: 80rpx;
  outline: solid 1px #888;
}
.register{
  width: 100%;
  height: 300rpx;
  margin: 15px 15px auto;
  color: white;
}