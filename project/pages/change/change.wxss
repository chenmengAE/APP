/* pages/change/change.wxss */
.coatain{
  width: 100%;
  height: 720px;
  margin: 0;
  padding: 0;
  position: relative;
}
.head{
  font-size: large;
  text-align: center;
  color: white;
  padding-top: 50rpx;
}
image{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.form{
  margin-top: 200rpx;
  margin-bottom: 100rpx;
}

label{
  display: flex;
  justify-content: flex-start;
  width: 720rpx;
  margin: 10rpx 0;
  margin-left: 15rpx;
}
label text{
  width: 150rpx;
  display: block;
  line-height: 80rpx;
  font-size: 18px;
  color: white;
}
label switch{
  margin: 5px 5px 0 63px;
}
label input{
  flex: 1;
  padding: 0 0 0 20rpx;
  height: 80rpx;
  outline: solid 1px #888;
}